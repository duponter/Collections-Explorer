<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">(?i).*-alpha.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-beta.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-rc.*</ignoreVersion>
        <ignoreVersion type="regex">(?i).*-m.*</ignoreVersion>
    </ignoreVersions>
    <rules>
        <rule groupId="org.apache.commons" artifactId="commons-lang3" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion>3.12.0.redhat-00001</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
